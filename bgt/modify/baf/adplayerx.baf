//Weapons

IF
  InParty(Myself)
  Global("ENDOFBG1","GLOBAL",0)
  HasItemEquipedReal("AX1H01",Myself) // Battle Axe
  Global("AX1H01","LOCALS",1)
THEN
  RESPONSE #50
    DisplayStringHead(Myself,@401115)
    SetGlobal("AX1H01","LOCALS",0)
    TakeItemReplace("BGMISC56","AX1H01",Myself)
  RESPONSE #50
    SetGlobal("AX1H01","LOCALS",0)
    Continue()
END

