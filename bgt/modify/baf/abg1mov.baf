IF
  Time(DUSK_END)
  !GlobalTimerNotExpired("A6Sunset","GLOBAL")
THEN
  RESPONSE #100
    StartMovie("BGSUNSET")
    SetGlobalTimer("A6Sunset","GLOBAL",ONE_HOUR)
END

IF
  Time(DAWN_END)
  !GlobalTimerNotExpired("A6Sunrise","GLOBAL")
THEN
  RESPONSE #100
    StartMovie("BGSUNRIS")
    SetGlobalTimer("A6Sunrise","GLOBAL",ONE_HOUR)
END
