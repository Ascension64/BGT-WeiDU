IF
  OnCreation()
  Global("laola","GLOBAL",1)
  Global("LaolaSpawn","GLOBAL",0)
  GlobalLT("Chapter","GLOBAL",8)
  !Exists("LAOLA")
THEN
  RESPONSE #100
    CreateCreature("LAOLA",[3656.1458],0)
